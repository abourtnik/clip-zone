{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "barryvdh/laravel-dompdf": "^3.0",
        "diglactic/laravel-breadcrumbs": "^10.0",
        "doctrine/dbal": "^3.6",
        "fakerphp/faker": "^1.9.1",
        "guzzlehttp/guzzle": "^7.2",
        "http-interop/http-factory-guzzle": "^1.2",
        "icehouse-ventures/laravel-chartjs": "^4.1",
        "kris/laravel-form-builder": "^1.52",
        "lab404/laravel-impersonate": "^1.7",
        "laravel/cashier": "^15.0",
        "laravel/framework": "^12.0",
        "laravel/horizon": "^5.21",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/socialite": "^5.8",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.7",
        "league/flysystem-aws-s3-v3": "^3.0",
        "maatwebsite/excel": "^3.1",
        "meilisearch/meilisearch-php": "^1.6",
        "opcodesio/log-viewer": "^3.1",
        "php-ffmpeg/php-ffmpeg": "^1.1",
        "predis/predis": "^2.2",
        "propaganistas/laravel-phone": "^6.0",
        "psr/simple-cache": "^2.0",
        "pusher/pusher-php-server": "^7.2",
        "spatie/eloquent-sortable": "^4.0",
        "staudenmeir/eloquent-has-many-deep": "^1.7",
        "staudenmeir/laravel-cte": "^1.11",
        "symfony/intl": "^6.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "barryvdh/laravel-ide-helper": "^v3.5.4",
        "larastan/larastan": "^v3.0.2",
        "laravel/pint": "^1.0",
        "mockery/mockery": "^1.5",
        "nunomaduro/collision": "^8.1",
        "phpunit/phpunit": "^11.5.3",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": false
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
